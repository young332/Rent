<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.rent.admin.mapper.AdBoardMapper">

	<delete id="deleteCheckedBoard">
        DELETE FROM rent001.TBL_BOARD
        WHERE board_no IN
        <foreach item="board_no" collection="list" open="(" separator="," close=")">
            #{board_no}
        </foreach>
    </delete>	


</mapper>